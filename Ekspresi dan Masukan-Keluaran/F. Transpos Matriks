#include <iostream>
using namespace std;

int main(){
    int matriks[3][3], transpos[3][3];
   
    for (int a = 0; a < 3; a++){
        for (int b = 0; b < 3; b++){
            cin>>matriks[a][b];
        }
        cout<<endl;
    }
    
    for (int a = 0; a < 3; a++){
        for (int b = 0; b < 3; b++){
            transpos[b][a] = matriks[a][b];
        }
        cout<<endl;
    }
    
    for (int a = 0; a < 3; a++){
        for (int b = 0; b < 3; b++){
            cout << transpos[a][b]<< "\t";
        }
        cout << endl;
    }
}
